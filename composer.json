{
    "name": "rjit/alumni-portal",
    "description": "RJIT Alumni Portal",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-pgsql": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.300",
        "firebase/php-jwt": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Config\\": "config/",
            "Helpers\\": "helpers/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
